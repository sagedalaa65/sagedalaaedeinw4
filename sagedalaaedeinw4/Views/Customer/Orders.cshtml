@model IEnumerable<sagedalaaedeinw4.Models.entities.Order>

<thead>
    <tr>
        <td>Id</td>
        <td>CustomerId</td>
        <td>OrderDate</td>
        <td>TotalAmount</td>
        <td>Status</td>
    </tr>
</thead>


<tbody>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.CustomerId</td>
            <td>@order.OrderDate</td>
            <td>@order.TotalAmount</td>
            <td>@order.Status</td>



        </tr>
    }

</tbody>
</table>